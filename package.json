    "eslint-plugin-unicorn": "^49.0.0",
    "husky": "8.0.3",
    "markdown-toc": "1.2.0",
    "npm-run-all": "4.1.5",
    "prettier": "^3.1.0",
    "release-it": "17.0.0",
    "syncpack": "11.2.1"
  },
  "engines": {
    "node": "^14.17.0 || ^16.0.0 || >= 18.0.0"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "6.12.0",
    "eslint": "^8.54.0",
